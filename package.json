{
  "name": "nuxt-og-image",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.8.0",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-og-image#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-og-image.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-og-image/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint \"**/*.{ts,vue,json,yml}\"",
    "build": "nuxi prepare .playground && nuxt-module-build",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare .playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "pnpm lint"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0",
    "chalk": "^5.2.0",
    "defu": "^6.1.1",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "ohash": "^1.0.0",
    "playwright": "^1.28.1",
    "radix3": "^1.0.0",
    "ufo": "^1.0.1"
  },
  "unbuild": {
    "externals": [
      "playwright",
      "playwright-core",
      "chrome-aws-lambda"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.33.1",
    "@nuxt/kit": "3.0.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/test-utils": "3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "bumpp": "^8.2.1",
    "chrome-aws-lambda": "^10.1.0",
    "eslint": "8.29.0",
    "nuxt": "npm:nuxt3@latest",
    "pathe": "^1.0.0",
    "playwright-chromium": "^1.28.1",
    "playwright-core": "^1.28.1",
    "vitest": "^0.25.5"
  }
}
